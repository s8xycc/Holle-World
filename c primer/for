#include <stdio.h>
int main(void)
{   
    int n;
    
    printf("    n    n cuded\n");
    for (n = 1; n<=3; n++)
    printf("%5d %5d\n");
    
    return 0;
}
//1.normal(print 1~5)
#include <stdio.h>
int main()
{
    int i;
    for(i=0;i<=5;++i)
    printf("%d",i);
    return 0;
}
 //1.normal(print 1~5,^2)
 #include <stdio.h>
 int main()
{
     int i,q;
     for(i=0,q=0;i<=5;++i,q=i*i)
     printf("%d,%d",i,q);
     return 0;
}
